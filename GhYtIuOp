-- Enlightened Users ESP Toggle
local eue = PlayersTab:CreateToggle({
    Name = "enlightened users ESP",
    CurrentValue = false,
    Flag = "eue",
    Callback = function(Value)
        getgenv().EnlightenedESP = Value
        if Value then
            -- Enable enlightened ESP
            spawn(function()
                while getgenv().EnlightenedESP do
                    for _, Player in ipairs(Players:GetPlayers()) do
                        if Player ~= LocalPlayer then
                            local Character = Player.Character
                            if Character then
                                local Humanoid = Character:FindFirstChild("Humanoid")
                                if Humanoid and Humanoid.Health > 0 then
                                    -- ESP logic
                                end
                            end
                        end
                    end
                    task.wait(0.1)
                end
            end)
        end
    end
})

-- Invisible Users ESP Toggle
PlayersTab:CreateToggle({
    Name = "invisible users ESP (best)",
    CurrentValue = false,
    Flag = "invis",
    Callback = function(Value)
        getgenv().InvisibleESP = Value
        if Value then
            spawn(function()
                while getgenv().InvisibleESP do
                    for _, Player in ipairs(Players:GetPlayers()) do
                        if Player ~= LocalPlayer then
                            local Character = Player.Character
                            if Character then
                                local Torso = Character:FindFirstChild("Torso")
                                if Torso and Torso.Transparency > 0 then
                                    -- Create ESP highlight
                                    local Highlight = Instance.new("Highlight")
                                    Highlight.Name = "invis"
                                    Highlight.FillColor = Color3.fromRGB(255, 0, 0)
                                    Highlight.Parent = Character

                                    -- Create text label
                                    local TextLabel = Instance.new("TextLabel")
                                    TextLabel.Text = "(INVIS USER: " .. Player.Name .. ")"
                                    TextLabel.TextScaled = true
                                    TextLabel.RichText = true
                                    TextLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                                    TextLabel.FontFace = Font.new("rbxasset://fonts/families/Arial.json", Enum.FontWeight.Bold)
                                    TextLabel.BackgroundTransparency = 1
                                    TextLabel.Size = UDim2.new(1, 0, 1, 0)
                                    TextLabel.Parent = Highlight
                                end
                            end
                        end
                    end
                    task.wait(0.1)
                end
            end)
        else
            -- Remove ESP
            for _, Player in ipairs(Players:GetPlayers()) do
                if Player ~= LocalPlayer then
                    local Character = Player.Character
                    if Character then
                        local Highlight = Character:FindFirstChild("invis")
                        if Highlight then
                            Highlight:Destroy()
                        end
                    end
                end
            end
        end
    end
})

