--===============================================
-- UTILITY FUNCTIONS
--===============================================

-- Raycast Parameters
local characterA = RaycastParams.new()
characterA.FilterType = Enum.RaycastFilterType.Exclude
characterA.FilterDescendantsInstances = {Character}

-- Rainbow Paint Loop
spawn(function()
    while true do
        if getgenv().RainbowPaint then
            local PaintTool = Character:FindFirstChild("Paint") or Backpack:FindFirstChild("Paint")
            if PaintTool then
                local Script = PaintTool:FindFirstChild("Script")
                if Script then
                    local Event = Script:FindFirstChild("Event")
                    if Event then
                        -- Rainbow paint logic
                        local hue = (tick() % 5) / 5
                        local color = Color3.fromHSV(hue, 1, 1)
                        -- Fire paint event
                    end
                end
            end
        end
        task.wait(0.1)
    end
end)

-- Delete Aura Loop
spawn(function()
    while true do
        if getgenv().DeleteAura then
            local DeleteTool = Backpack:FindFirstChild("Delete")
            if DeleteTool then
                local Script = DeleteTool:FindFirstChild("Script")
                if Script then
                    local Event = Script:FindFirstChild("Event")
                    if Event then
                        -- Get nearest HRP
                        local NearestHRP = nil
                        local MinDistance = math.huge

                        for _, Player in ipairs(Players:GetPlayers()) do
                            if Player ~= LocalPlayer then
                                local Character = Player.Character
                                if Character then
                                    local HumanoidRootPart = Character:FindFirstChild("HumanoidRootPart")
                                    if HumanoidRootPart then
                                        local Distance = (HumanoidRootPart.Position - Character.HumanoidRootPart.Position).Magnitude
                                        if Distance < MinDistance then
                                            MinDistance = Distance
                                            NearestHRP = HumanoidRootPart
                                        end
                                    end
                                end
                            end
                        end

                        if NearestHRP then
                            -- Create delete aura part
                            local DeleteAuraPart = Instance.new("Part")
                            DeleteAuraPart.Size = Vector3.new(10, 10, 10)
                            DeleteAuraPart.Parent = Workspace
                            DeleteAuraPart.Transparency = 0.5
                            DeleteAuraPart.Color = Color3.fromRGB(255, 0, 0)
                            DeleteAuraPart.Material = Enum.Material.Neon
                            DeleteAuraPart.Name = "DeleteAura"
                            DeleteAuraPart.Anchored = true
                            DeleteAuraPart.CanCollide = false

                            -- Get parts in bounds
                            local OverlapParams = OverlapParams.new()
                            OverlapParams.FilterType = Enum.RaycastFilterType.Exclude
                            OverlapParams.FilterDescendantsInstances = {Workspace.Bricks}

                            local Parts = Workspace:GetPartBoundsInBox(CFrame.new(NearestHRP.Position), Vector3.new(10, 10, 10), OverlapParams)

                            for _, Part in ipairs(Parts) do
                                if Part:IsA("BasePart") and not Part:IsDescendantOf(Workspace.Bricks) then
                                    Event:FireServer(unpack({Part.Position, "both", Color3.fromHSV((tick() % 5) / 5, 1, 1), "spray"}))
                                end
                            end
                        end
                    end
                end
            end
        end
        task.wait(0.1)
    end
end)

-- Rainbow Sky Loop
spawn(function()
    while true do
        if getgenv().RainbowSky then
            -- Rainbow sky logic
            local hue = (tick() % 10) / 10
            -- Apply sky color
        end
        task.wait(0.1)
    end
end)

-- Block Selection
getgenv().SelectedBlock = nil

-- Notify user
Rayfield:Notify({
    Title = "Script Loaded",
    Content = "Hunterstorm's unlisted t v" .. CurrentVersion .. " loaded successfully!",
    Duration = 5,
    Image = "check"
})

print("Script loaded successfully!")
