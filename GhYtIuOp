-- Delcubes All Button
MainTab:CreateButton({
    Name = "Delcubes all",
    Callback = function()
        -- Delete all cubes
        for _, Part in ipairs(Workspace.Bricks:GetDescendants()) do
            if Part:IsA("BasePart") then
                local DeleteTool = Backpack:FindFirstChild("Delete")
                if DeleteTool then
                    local Script = DeleteTool:FindFirstChild("Script")
                    if Script then
                        local Event = Script:FindFirstChild("Event")
                        if Event then
                            Event:FireServer(unpack({Part.Position, "both", Color3.new(1, 1, 1), "spray"}))
                        end
                    end
                end
            end
        end
    end
})

